# IB Gateway Docker Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# VNC password for remote desktop access (required)
VNC_PASSWORD=

# Interactive Brokers credentials (required)
TWSUSERID=
TWSPASSWORD=

# Trading mode: 'paper' or 'live' (required)
TRADING_MODE=paper

# FIX credentials (optional - leave empty if not using FIX)
FIXUSERID=
FIXPASSWORD=

# Java path override (optional - usually auto-detected)
# JAVA_PATH=/opt/i4j_jres/1.8.0_152-tzdata2019c/bin
